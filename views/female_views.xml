<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_lawyers_female_action" model="ir.actions.act_window">
        <field name="name">lawyers female model</field>
        <field name="res_model">lawyers.second</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_gender': 'male'}</field>
        <field name="domain">[('gender','=','female')]</field>
    </record>

    <menuitem name="lawyer third female menu"
              id="lawyer_third_female_menu"
              sequence="2"
              parent="lawyer_second_menu"
              action="view_lawyers_female_action"/>
</odoo>

